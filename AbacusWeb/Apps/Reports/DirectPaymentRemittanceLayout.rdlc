  <Code>        Public Shared _InvNo As String
        Public Shared _InvDate As String
        Public Shared _InvNoLabel As String
        Public Shared _InvDateLabel As String

        Public Shared function setFooterText(ByVal invNumberLabel As String, ByVal invNumber As String, _
                                        ByVal invDateLabel As String, ByVal invDate As String) as String
            _InvNo = invNumber
            _InvDate = invDate
            _InvNoLabel = invNumberLabel
            _InvDateLabel = invDateLabel
            Return ""
        End Function

        Public Shared Function getFooterText() As String
            Return String.Format("{0} : {1}{2}{3} : {4}", _InvNoLabel, _InvNo, vbcrlf ,_InvDateLabel, _InvDate)
        End Function</Code>
